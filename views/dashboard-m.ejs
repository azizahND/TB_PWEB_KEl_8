    <div class="container">
      <h1>Data Evaluasi</h1>
      <table id="kriteriaTable">
          <thead>
              <tr>
                  <th>No</th>
                  <th>Tanggal</th>
                  <th>Evaluasi</th>
                
              </tr>
          </thead>
          <tbody>
              <!-- Baris-baris tabel akan ditambahkan secara dinamis -->
          </tbody>
      </table>
      <button class="btn" onclick="addRow()">Tambah Baris</button>
  </div>

  <script>
      function addRow() {
          const table = document.getElementById('kriteriaTable').getElementsByTagName('tbody')[0];
          const rowCount = table.rows.length;
          const row = table.insertRow(rowCount);

          const cell1 = row.insertCell(0);
          const cell2 = row.insertCell(1);
          const cell3 = row.insertCell(2);
          

          cell1.innerHTML = rowCount + 1;
          cell2.innerHTML = '<input type="text" name="tanggal">';
          cell3.innerHTML = '<input type="text" name="namaMahasiswa">';
         
      }

      
  </script>
    
    
