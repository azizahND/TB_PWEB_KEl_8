<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Dashboard Mahasiswa</title>
    <link href="/styles/dashboard.css" rel="stylesheet" />
    <script src="/socket.io/socket.io.js"></script>
</head>
<body>
    <div class="nav">
        <ul class="menu">
            <li><a href="#"><img src="/images/unand.jpeg" alt="Logo"></a></li>
            <li><a href="#" class="active">Dashboard</a></li>
            <li><a href="#">Evaluasi</a></li>
            <li><a href="#">Umpan Balik</a></li>
            <li><a href="#">Notifikasi</a></li>
            <li><a href="/mahasiswa/profil" >Profile</a></li>
            <li><a href="/login" style="margin-top: 70px;">Log Out</a></li>
        </ul>
    </div>

    <div class="container">
        <h1>Data Evaluasi</h1>
        <table id="kriteriaTable">
            <thead>
                <tr>
                    <th style="text-align: center;">No</th>
                    <th style="text-align: center;">Tanggal</th>
                    <th style="text-align: center;">Evaluasi</th>
                </tr>
            </thead>
            <tbody>
                <% if (evaluasiJawaban && evaluasiJawaban.length > 0) { %>
                    <% evaluasiJawaban.forEach((jawaban, index) => { %>
                        <tr>
                            <td><%= index + 1 %></td>
                            <td><%= jawaban.createdAt.toLocaleDateString() %></td>
                            <td>
                                <ul>
                                    <% jawaban.detailJawabanEvaluasi.forEach((detail) => { %>
                                        <li><%= detail.pertanyaan.pertanyaan %>: <%= detail.jawaban %></li>
                                    <% }) %>
                                </ul>
                            </td>
                        </tr>
                    <% }); %>
                <% } else { %>
                    <tr>
                        <td colspan="3">Tidak ada data evaluasi.</td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>

    
    
</body>

</html>
